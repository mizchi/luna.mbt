{
  "name": "@luna_ui/playwright-chaos",
  "version": "0.2.7",
  "description": "Playwright-based chaos testing tool for web applications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "chaos-crawler": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./fixture": {
      "types": "./dist/fixture.d.ts",
      "import": "./dist/fixture.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "lint": "biome check src/",
    "crawl": "tsx src/cli.ts"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "playwright",
    "playwright-test",
    "testing",
    "chaos-engineering",
    "monkey-testing",
    "accessibility",
    "crawler"
  ],
  "peerDependencies": {
    "@playwright/test": "^1.40.0",
    "playwright": "^1.40.0"
  },
  "peerDependenciesMeta": {
    "@playwright/test": {
      "optional": true
    }
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/node": "^22.0.0",
    "playwright": "^1.52.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.0.7"
  }
}
