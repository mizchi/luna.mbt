{
  "name": "@luna_ui/shoelace",
  "version": "0.3.5",
  "description": "Shoelace component bindings for Luna",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./button": {
      "types": "./dist/button.d.mts",
      "import": "./dist/button.mjs"
    },
    "./input": {
      "types": "./dist/input.d.mts",
      "import": "./dist/input.mjs"
    },
    "./checkbox": {
      "types": "./dist/checkbox.d.mts",
      "import": "./dist/checkbox.mjs"
    },
    "./switch": {
      "types": "./dist/switch.d.mts",
      "import": "./dist/switch.mjs"
    }
  },
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsdown",
    "dev": "pnpm build && npx serve ."
  },
  "peerDependencies": {
    "@shoelace-style/shoelace": "^2.20.0"
  },
  "devDependencies": {
    "tsdown": "^0.18.2"
  },
  "repository": {
    "directory": "js/luna-shoelace",
    "url": "https://github.com/mizchi/luna.mbt"
  },
  "files": [
    "dist",
    "src"
  ]
}
