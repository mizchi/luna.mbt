{
  "name": "@mizchi/kaguya-loader",
  "version": "0.1.0",
  "description": "Island loader for Kaguya SSR hydration",
  "type": "module",
  "main": "loader.js",
  "exports": {
    ".": "./loader.js",
    "./min": "./loader.min.js",
    "./v1": "./kg-loader-v1.js"
  },
  "files": [
    "loader.js",
    "loader.min.js",
    "kg-loader-v1.js"
  ],
  "scripts": {
    "minify": "terser loader.js --module --compress --mangle -o loader.min.js",
    "minify:v1": "terser kg-loader-v1.js --module --compress --mangle -o kg-loader-v1.min.js"
  },
  "keywords": [
    "kaguya",
    "moonbit",
    "island",
    "ssr",
    "hydration"
  ],
  "author": "mizchi",
  "license": "MIT"
}
