{
  "supported-targets": [
    "js"
  ],
  "import": [
    { "path": "mizchi/luna/core", "alias": "luna" },
    "mizchi/luna/core/signal",
    { "path": "mizchi/luna/platform/dom", "alias": "dom" },
    { "path": "mizchi/luna/platform/dom/client", "alias": "client" },
    { "path": "mizchi/js/core", "alias": "js" },
    { "path": "mizchi/js/browser/dom", "alias": "js_dom" }
  ],
  "test-import": [
    "mizchi/npm_typed/global_jsdom"
  ],
  "link": {
    "js": {
      "exports": [
        "create_signal:createSignal",
        "get",
        "set",
        "update",
        "peek",
        "subscribe",
        "map",
        "create_memo:createMemo",
        "combine",
        "effect",
        "batch_start:batchStart",
        "batch_end:batchEnd",
        "run_untracked:runUntracked",
        "batch",
        "on_cleanup:onCleanup",
        "create_root:createRoot",
        "get_owner:getOwner",
        "run_with_owner:runWithOwner",
        "has_owner:hasOwner",
        "on_mount:onMount",
        "define"
      ],
      "format": "esm"
    }
  }
}
