    { "path": "mizchi/luna/core", "alias": "luna" },
    "mizchi/luna/core/signal",
    { "path": "mizchi/luna/router", "alias": "router" },
    { "path": "mizchi/luna/platform/dom/element", "alias": "dom" },
    { "path": "mizchi/luna/platform/dom", "alias": "browser" },
    { "path": "mizchi/luna/platform/dom/client", "alias": "client" },
